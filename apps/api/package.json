{
  "name": "api",
  "version": "0.0.1",
  "private": true,
  "nx": {
    "targets": {
      "tailwind:build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "pnpx @tailwindcss/cli -i ./views/global.css -o ./assets/main.css",
          "cwd": "{projectRoot}"
        }
      },
      "build": {
        "executor": "nx:run-commands",
        "outputs": [
          "{projectRoot}/dist"
        ],
        "options": {
          "command": "nest build",
          "cwd": "{projectRoot}"
        },
        "dependsOn": [
          "^build"
        ]
      },
      "prune-lockfile": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "executor": "@nx/js:prune-lockfile",
        "outputs": [
          "{workspaceRoot}/apps/api/dist/package.json",
          "{workspaceRoot}/apps/api/dist/pnpm-lock.yaml"
        ],
        "options": {
          "buildTarget": "build"
        }
      },
      "copy-workspace-modules": {
        "dependsOn": [
          "build"
        ],
        "cache": true,
        "outputs": [
          "{workspaceRoot}/apps/api/dist/workspace_modules"
        ],
        "executor": "@nx/js:copy-workspace-modules",
        "options": {
          "buildTarget": "build"
        }
      },
      "prune": {
        "dependsOn": [
          "prune-lockfile",
          "copy-workspace-modules"
        ],
        "executor": "nx:noop"
      },
      "serve": {
        "continuous": true,
        "executor": "nx:run-commands",
        "options": {
          "command": "nest start --watch",
          "cwd": "{projectRoot}"
        }
      },
      "docker:build": {
        "dependsOn": [
          "build",
          "prune"
        ]
      },
      "codegen": {
        "executor": "nx:run-commands",
        "options": {
          "command": "kysely-codegen",
          "cwd": "{projectRoot}"
        }
      },
      "migrate": {
        "executor": "nx:run-commands",
        "options": {
          "command": "ts-node -P tsconfig.app.json src/migrate.ts",
          "cwd": "{projectRoot}"
        }
      },
      "migrate:reset": {
        "executor": "nx:run-commands",
        "options": {
          "command": "ts-node -P tsconfig.app.json src/migrate.ts --reset",
          "cwd": "{projectRoot}"
        }
      },
      "gen-jwks": {
        "executor": "nx:run-commands",
        "options": {
          "command": "ts-node -P tsconfig.app.json src/gen-jwks.ts",
          "cwd": "{projectRoot}"
        }
      },
      "bootstrap": {
        "executor": "nx:run-commands",
        "options": {
          "command": "bash script/bootstrap.sh",
          "cwd": "{projectRoot}"
        }
      }
    }
  },
  "dependencies": {
    "@aws-crypto/client-node": "^4.2.1",
    "@aws-sdk/client-bedrock-runtime": "^3.931.0",
    "@aws-sdk/credential-providers": "^3.931.0",
    "@fastify/express": "^4.0.2",
    "@fastify/static": "^8.3.0",
    "@fastify/view": "11.1.1",
    "@keyv/redis": "^5.1.3",
    "@nest-lab/or-guard": "^2.6.0",
    "@nestjs/cache-manager": "^3.0.1",
    "@nestjs/common": "^11.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^11.0.0",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-fastify": "^11.1.7",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/swagger": "^11.2.1",
    "@smithy/types": "^4.9.0",
    "@vm-x-ai/shared-ai-provider": "workspace:*",
    "argon2": "^0.44.0",
    "axios": "^1.13.2",
    "axios-retry": "^4.5.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.2",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "ejs": "^3.1.10",
    "fastify": "^5.6.1",
    "ioredis": "^5.8.2",
    "joi": "^18.0.1",
    "js-tiktoken": "^1.0.21",
    "json-schema": "^0.4.0",
    "keyv": "^5.5.3",
    "kysely": "^0.28.8",
    "kysely-codegen": "0.19.0",
    "lodash": "4.17.21",
    "lodash.get": "^4.4.2",
    "lodash.merge": "^4.6.2",
    "nanoid": "^5.1.6",
    "nestjs-pino": "^4.4.1",
    "node-jose": "^2.2.0",
    "node-vault": "^0.10.9",
    "oidc-provider": "^9.5.2",
    "openai": "^6.7.0",
    "openid-client": "^6.8.1",
    "passport-http-bearer": "^1.0.1",
    "pg": "^8.16.3",
    "pino": "^10.1.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.2",
    "string-dedent": "^3.0.2",
    "ts-enum-util": "^4.1.0",
    "uuid": "^13.0.0",
    "yargs": "18.0.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^11.0.0",
    "@tailwindcss/cli": "^4.1.17",
    "@tailwindcss/vite": "^4.1.17",
    "@types/ejs": "^3.1.5",
    "@types/express": "^5.0.4",
    "@types/json-schema": "^7.0.15",
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.merge": "^4.6.9",
    "@types/node-jose": "^1.1.13",
    "@types/oidc-provider": "^9.5.0",
    "@types/passport-http-bearer": "^1.0.42",
    "@types/yargs": "^17.0.34",
    "kysely-codegen": "^0.19.0",
    "tailwindcss": "4.1.17",
    "vite": "^7.1.12",
    "yargs": "^18.0.0"
  }
}