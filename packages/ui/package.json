{
  "name": "ui",
  "version": "0.1.0",
  "private": true,
  "nx": {
    "name": "ui",
    "tags": [
      "ui"
    ],
    "release": {
      "docker": {
        "repositoryName": "vm-x-ai/ui"
      }
    },
    "targets": {
      "build": {
        "executor": "nx:run-commands",
        "options": {
          "command": "next build",
          "cwd": "{projectRoot}"
        },
        "dependsOn": [
          "^build"
        ],
        "cache": false
      },
      "gen-client": {
        "executor": "nx:run-commands",
        "options": {
          "command": "pnpx @hey-api/openapi-ts -f openapi-ts.api.config.ts",
          "cwd": "{projectRoot}"
        }
      },
      "docker:build": {
        "dependsOn": [
          "build"
        ]
      }
    }
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.71",
    "@ai-sdk/react": "^2.0.101",
    "@hookform/resolvers": "^5.2.2",
    "@monaco-editor/react": "^4.7.0",
    "@mui/icons-material": "^7.3.5",
    "@mui/lab": "7.0.1-beta.19",
    "@mui/material": "^7.3.5",
    "@mui/material-nextjs": "^7.3.5",
    "@mui/x-date-pickers": "^8.19.0",
    "@nivo/core": "^0.99.0",
    "@nivo/line": "^0.99.0",
    "@nivo/scales": "^0.99.0",
    "@nivo/theming": "^0.99.0",
    "@tanstack/react-query": "^5.90.10",
    "@tanstack/react-query-devtools": "^5.90.2",
    "@vercel/otel": "^2.1.0",
    "@vm-x-ai/shared-ai-provider": "workspace:*",
    "ace-builds": "^1.43.4",
    "ai": "^5.0.101",
    "ajv": "^8.17.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "dayjs": "^1.11.19",
    "dnd-core": "^16.0.1",
    "ejs": "^3.1.10",
    "http-status-codes": "^2.3.0",
    "immutability-helper": "^3.1.1",
    "json-schema": "^0.4.0",
    "material-react-table": "^3.2.1",
    "monaco-editor": "^0.55.1",
    "next": "^16.0.3",
    "next-auth": "5.0.0-beta.30",
    "nuqs": "^2.8.1",
    "react": "^19.0.0",
    "react-ace": "^14.0.1",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^19.0.0",
    "react-hook-form": "^7.66.1",
    "react-json-view": "^1.21.3",
    "react-markdown": "^10.1.0",
    "react-spinners": "^0.17.0",
    "react-syntax-highlighter": "^16.1.0",
    "react-toastify": "^11.0.5",
    "remark-gfm": "^4.0.1",
    "string-dedent": "^3.0.2",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.17",
    "titleize": "^4.0.0",
    "tw-animate-css": "^1.4.0",
    "zod": "^4.1.12",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.88.0",
    "@tailwindcss/postcss": "^4.1.17",
    "@types/ejs": "^3.1.5",
    "@types/json-schema": "^7.0.15",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@types/shade-blend-color": "^1.0.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  }
}
